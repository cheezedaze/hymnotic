{
  "CORSRules": [
    {
      "AllowedHeaders": ["*"],
      "AllowedMethods": ["GET", "HEAD", "PUT", "POST"],
      "AllowedOrigins": [
        "http://localhost:3000",
        "http://localhost:3333",
        "https://hymnotic.com",
        "https://www.hymnotic.com",
        "https://*.vercel.app"
      ],
      "ExposeHeaders": [
        "Content-Length",
        "Content-Range",
        "Accept-Ranges",
        "ETag"
      ],
      "MaxAgeSeconds": 86400
    }
  ]
}
